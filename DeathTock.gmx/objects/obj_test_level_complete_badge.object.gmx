        lightui_set_font_color(other.number_color);
    }

    // Coin line
    lightui_create_label(40, 105, 'Coins');
    lightui_create_label(125, 105, ':');
    global.coins_label = lightui_create_label_right_align(280, 105, 350);
    with(global.coins_label)
    {
        lightui_set_font_color(other.number_color);
    }

    // Bonus line
    lightui_create_label(40, 150, 'Bonus');
    lightui_create_label(125, 150, ':');
    global.bonus_label = lightui_create_label_right_align(280, 150, 1600);
    with(global.bonus_label)
    {
        lightui_set_font_color(other.number_color);
    }
    
    // Add icons to the bottom of the window
    lightui_create_icon_button(25, 230, 23, 'test_home', menu_button_demo_callback);
    lightui_create_icon_button(noone, 230, 9, 'test_home', menu_button_demo_callback);
    lightui_create_icon_button(215, 230, 18, 'test_home', menu_button_demo_callback);
}

