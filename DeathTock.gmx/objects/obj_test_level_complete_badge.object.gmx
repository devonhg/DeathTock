with(lightui_create_window(noone, 360, 320, 280, ''))
{
    // Create a custom color for the score numbers
    number_color = make_colour_rgb(244, 249, 35);

    // Create the level badge
    lightui_create_level_complete_badge(noone, -275, 1, '12');

    // Score line
    lightui_create_label(40, 60, 'Score');
    lightui_create_label(125, 60, ':');
    global.score_label = lightui_create_label_right_align(280, 60, 0);
    with(global.score_label)
